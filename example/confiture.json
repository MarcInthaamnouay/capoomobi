{
  "confitures": [
    {
      "name": "web",
      "deployment": {
        "replicas": 10,
        "controller": "Deployment"
      },
      "service": {
        "kind": "NodePort",
        "nodeport": 30302
      }
    },
    {
      "name": "mysql",
      "deployment": {
        "replicas": 10,
        "controller": "StatefulSet"
      },
      "service": {
        "kind": "ClusterIP"
      }
    },
    {
      "name": "front",
      "deployment": {
        "replicas": 10,
        "controller": "Deployment"
      },
      "service": {
        "kind": "NodePort",
        "nodeport": 30302
      }
    }
  ],
  "ingress": {
    "ip": "30.10.20.30",
    "services": [
      {
        "name": "web",
        "path": "/web"
      },
      {
        "name": "front",
        "path": "/front"
      }
    ]
  }
}